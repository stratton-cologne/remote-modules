{
    "compilerOptions": {
        "target": "ES2021",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "lib": ["ES2021", "DOM"],
        "strict": true,
        "declaration": true,
        "declarationMap": false,
        "sourceMap": false,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "types": []
    },
    "include": ["src"]
}
